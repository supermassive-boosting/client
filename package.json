{
  "name": "supermassive",
  "version": "0.0.1",
  "main": "dist-electron/main/index.js",
  "description": "Supermassive desktop application",
  "author": "Yankı Küçük <kendinikertenkelebek@me.com>",
  "license": "Apache-2.0",
  "private": true,
  "keywords": [
    "supermassive",
    "community",
    "world of warcraft",
    "boosting",
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "type": "module",
  "scripts": {
    "web": "",
    "dev": "vite --config vite.config.ts",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "clean": "git clean -xdf node_modules dist dist-electron",
    "lint": "eslint . --cache --ext .ts,.js,.vue && prettier . --check --cache",
    "lint:fix": "eslint . --ext .ts,.js,.vue --fix && prettier . --write",
    "preview": "vite preview"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@vueuse/core": "^10.11.0",
    "axios": "^1.7.3",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.2.1",
    "less": "^4.2.0",
    "marked": "^13.0.3",
    "pinia": "^2.2.0",
    "turndown": "^7.2.0",
    "uikit": "^3.21.9",
    "vue": "^3.4.35",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.4.2"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^13.0.0",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-vite": "^2.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-resolver": "^1.0.3",
    "vue-tsc": "^2.0.29"
  }
}
